package lesson;

class Point {
	int x;
	int y;

	Point(int x, int y) {
		System.out.println("Point のコンストラクタが呼び出されました");
		this.x = x;
		this.y = y;

	}
}

public class MethodCallExample {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ

		Point p = new Point (5,10);
		System.out.println("Pointの座標は、(" + p.x +","+ p.y +")");
		
		
	}
}